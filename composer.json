{
  "name": "josh/laravel-phantomjs",
  "description": "Use phantom-js in laravel",
  "keywords": [
    "phantomjs-php",
    "phantomjs",
    "phantom-js",
    "laravel"
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/MrJoshLab/laravel-phantomjs/issues",
    "source": "https://github.com/MrJoshLab/laravel-phantomjs"
  },
  "authors": [
    {
      "name": "Alireza Josheghani",
      "email": "josheghani.dev@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Josh\\Component\\PhantomJs\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  {
    "type": "vcs",
    "url": "https://github.com/metallurgical/php-phantomjs"
  },
  "require": {
    "php": "^7.2",
    "laravel/framework": "^6.0|^7.0",
    "josh/php-phantomjs": "dev-laravel-7"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0"
  },
  "extra": {
    "laravel": {
        "providers": [    
          "Josh\\Component\\PhantomJs\\PhantomJsServiceProvider"
        ],
        "aliases": {
            "PhantomJs": "Josh\\Component\\PhantomJs\\Facade\\PhantomJs"
        }
    }
  }
}
